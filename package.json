{
  "name": "bgg-games-ranks",
  "version": "0.1.0",
  "description": "bgg games ranks parser",
  "main": "dist",
  "engines": {
    "node": ">=12.6",
    "npm": ">=6.9"
  },
  "dependencies": {
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "xmldom": "0.6.0",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@now/node": "1.7.1",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.9",
    "@types/xmldom": "0.1.31",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4",
    "vercel": "28.1.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts*\" \"test*/**/*.ts*\"",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json --coverage",
    "start:init": "./node_modules/.bin/vercel",
    "start:dev": "./node_modules/.bin/vercel dev",
    "update": "curl 'http://localhost:3000/api/update-local-file?amount=2000&load'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zinovik/bgg-games-ranks.git"
  },
  "author": "Max Zinovik",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zinovik/bgg-games-ranks/issues"
  },
  "homepage": "https://github.com/zinovik/bgg-games-ranks#readme"
}
